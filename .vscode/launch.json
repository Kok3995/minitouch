    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "preLaunchTask": "Forward debug port",
            "targetArchitecture": "x86",
            "program": "${workspaceFolder}/obj/local/x86/minitouch",
            "additionalSOLibSearchPath": "C:/Users/${env:USERNAME}/AppData/Local/Android/Sdk/ndk/19.2.5345600/platforms/android-19/arch-arm/usr/lib",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "miDebuggerPath": "C:/Users/${env:USERNAME}/AppData/Local/Android/Sdk/ndk/19.2.5345600/prebuilt/windows-x86_64/bin/gdb.exe",
            "miDebuggerServerAddress": "localhost:5039",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}